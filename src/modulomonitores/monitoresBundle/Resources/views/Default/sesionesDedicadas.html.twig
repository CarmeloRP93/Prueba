{# empty Twig template #}
{% extends 'modulomonitoresmonitoresBundle::main.html.twig' %}

{% block title %} Vista de sesiones dedicadas {% endblock %}            
{% block contenido %}

    <h1>Listado de sesiones de dedicadas</h1>
    {% for sesion in sesiones %}
        {% if sesion.cliente!=null %}
        <a href="{{url('modulomonitores_monitores_sesionDedicada', {'id': sesion.id})}}" class="fila {{cycle(["par","impar"], loop.index)}}"> Sesion {{ sesion.id }}: {{ sesion.cliente }}/{{ sesion.nombre }} - {{ sesion.estado}} </a>
    {% endif %}
    {% endfor %}
    <div class="accionesesion text-center" style="margin-top:5px; margin-left: 5px">
        <a href="{{path('modulomonitores_monitores_nuevaSesion')}}" class="btn btn-success col-sm-1" style="height: 30px; width: 150px;">Nuevo sesion <span class="glyphicon glyphicon-plus"></span></a>
    </div>
{% endblock %}